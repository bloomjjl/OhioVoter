@model OhioVoter.ViewModels.Candidate.CandidateCompareCaucusFirstViewModel

@{ if (Model.CandidateDisplayId == Model.CandidateId)
    {
        foreach (var line in @Model.CandidateCaucusHistory)
        {
            <p>@line</p>
        }
    }
    else if (Model.CandidateDisplayId == Model.RunningMateId)
    {
        foreach (var line in @Model.RunningMateCaucusHistory)
        {
            <p>@line</p>
        }
    }
}
