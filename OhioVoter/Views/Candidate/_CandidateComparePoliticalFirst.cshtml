@model OhioVoter.ViewModels.Candidate.CandidateComparePoliticalFirstViewModel

@{ if (Model.CandidateDisplayId == Model.CandidateId)
    {
        foreach (var line in @Model.CandidatePoliticalHistory)
        {
            <p>@line</p>
        }
    }
    else if (Model.CandidateDisplayId == Model.RunningMateId)
    {
        foreach (var line in @Model.RunningMatePoliticalHistory)
        {
            <p>@line</p>
        }
    }
}

