@model OhioVoter.ViewModels.Ballot.BallotViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Ballot Page";
}

<div class="row">

    <div id="side-bar" class="col-sm-4">
        @{Html.RenderAction("DisplaySideBar", "Location", Model.ControllerName as string);}
    </div>

    <div class="col-sm-8">

        <div class="panel-page-group">
            <h2><p class="panel-page-title"> My Ballot </p></h2>
        </div>

        <div id="ballot_display">
            @{Html.RenderAction("DisplayBallotInformation", "Ballot", Model);}
        </div>

    </div>

</div>


