@model OhioVoter.ViewModels.Home.HomeViewModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Home Page";
}

<div class="row">

    <div id="side-bar" class="col-sm-4">
        @{Html.RenderAction("DisplaySideBar", "Location");}
    </div>

    <div class="col-sm-8">

        <div class="calendar-group">
            <h2><p class="panel-page-title"> Voter Bulletin Board </p></h2>
        </div>

        <div class="election-calendar">
            @{Html.RenderPartial("_Calendar", Model.CalendarViewModel);}
        </div>

        <div class="election-poll">
            @{Html.RenderPartial("_Poll", Model.PollViewModel);}
        </div>

        <div class="rss-news">
           

            @{Html.RenderPartial("_RssFeeds", Model.RssFeedsViewModel);}
           
        </div>

    </div>

</div>
